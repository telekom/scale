<script src="/symbol_names.js"></script>
<script>
  Array.from(document.querySelectorAll('scale-textarea')).forEach((btn, i) => {
    btn.dataset.sketchSymbol = `Text Area / ${order(
      1,
      getCategoryName(btn)
    )} / ${order(2, btn.hasAttribute('helper-text') ? 'Info' : 'Standard')}${
      btn.hasAttribute('helper-text')
        ? ' / ' +
          order(
            3,
            btn.getAttribute('status') === 'error' ? 'Error' : 'Standard'
          )
        : ''
    } / ${order(
      4,
      btn.getAttribute('counter') === 'true' ? 'Character Count' : 'Standard'
    )} / ${order(
      5,
      btn.hasAttribute('value') ? 'Has Input Value' : 'Standard'
    )} / ${order(6, getStateName(btn))}`;
  });
</script>
