<script src="/symbol_names.js"></script>
<script>
  const symbols = [];
  Array.from(document.querySelectorAll('scale-text-field')).forEach(
    (btn, i) => {
      const symbolName = `Text Field / ${order(1, getSizeName(btn))} / ${order(
        2,
        evalNaming(btn)
      )} / ${order(
        3,
        btn.getAttribute('counter') === 'true' ? 'Character Count' : 'Standard'
      )} / ${order(
        4,
        btn.hasAttribute('value') ? 'Input Value' : 'Standard'
      )} / ${order(5, getStateName(btn))}`;

      symbols.push(symbolName);
      btn.dataset.sketchSymbol = symbolName;
    }
  );

  console.log(symbols);
</script>
