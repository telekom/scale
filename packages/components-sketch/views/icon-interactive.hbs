<script>
  const sizeNames = {
    48: 'Large',
    24: 'Medium',
    16: 'Small',
  };
  Array.from(document.querySelectorAll('scale-button')).forEach((btn, i) => {
    btn.dataset.sketchSymbol = `Icon / 02 Interactive / ${order(
      2,
      getCategoryName(btn)
    )} / ${order(3, sizeNames[btn.getAttribute('icon-size')])} / ${order(
      4,
      getStateName(btn)
    )}`;
  });
</script>
