<div data-sketch-artboard="Icon Static" class="artboard">
    <h1 class="title--artboard">Icon Static</h1>

    <h2 class="title--artboard-section">Large</h2>
    <div class="agent-states--grid">
        <div class="agent-states--item">
            <p class="agent-states--label">:available</p>
            <div class="icon-container" data-sketch-key="icon-static-1">
                <scale-icon path="{{>icon}}" size="48"/>
            </div>
        </div>
    </div>

    <h2 class="title--artboard-section">Medium</h2>
    <div class="agent-states--grid">
        <div class="agent-states--item">
            <p class="agent-states--label">:available</p>
            <div class="icon-container" data-sketch-key="icon-static-2">
                <scale-icon path="{{>icon}}" size="24"/>
            </div>
        </div>
    </div>

    <h2 class="title--artboard-section">Small</h2>
    <div class="agent-states--grid">
        <div class="agent-states--item">
            <p class="agent-states--label">:available</p>
            <div class="icon-container" data-sketch-key="icon-static-3">
                <scale-icon path="{{>icon}}" size="16"/>
            </div>
        </div>
    </div>
</div>

<script src="/symbol_names.js"></script>
<script>
    const sizeNames = {
        48: 'Large',
        24: 'Medium',
        16: 'Small'
    };
    Array.from(document.querySelectorAll(".icon-container")).forEach(btn => {
        btn.dataset.sketchSymbol = `Icon / 01 Static / ${
                order(2, sizeNames[btn.firstElementChild.getAttribute('size')])
            }`;
    });
</script>
