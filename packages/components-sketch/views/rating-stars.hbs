<script src="/placeCursor.js"></script>
<script>
  function assignPointers() {
    Array.from(document.querySelectorAll('scale-rating-stars'))
      .filter((component) => {
        if (['hover', 'active'].includes(component.dataset.fakeState))
          return component;
        if (['hover', 'active'].includes(component.dataset.sketchState))
          return component;
      })
      .forEach((component) => {
        const rating = component.rating;

        if (rating > 0) {
          const targetStar = component.shadowRoot.querySelectorAll(
            '[part="star"]'
          )[rating - 1];
          placeCursor(targetStar, { left: '40%', top: '60%' });
        }
      });
  }

  function onBeforePage2Layers() {
    assignPointers();

    Array.from(document.querySelectorAll('scale-rating-stars'))
      .filter((component) => component.dataset.sketchState !== 'focus')
      .forEach((component) => {
        component.shadowRoot.querySelector('[part="wrapper"]').style.boxShadow =
          '0 0 0 2px transparent';
      });
  }

  Array.from(document.querySelectorAll('scale-rating-stars')).forEach(
    (component, i) => {
      function isReadonly() {
        return component.hasAttribute('readonly');
      }
      function isLabelHidden() {
        return component.hasAttribute('hide-label');
      }

      function getSize() {
        return order(
          3,
          component.getAttribute('size') === 'large' ? 'Large' : 'Small'
        );
      }

      function getType() {
        return order(2, isReadonly() ? 'Static' : 'Interactive');
      }

      function getHidden() {
        return order(1, isLabelHidden() ? 'Hidden Label' : 'Standard');
      }

      function getRating() {
        const value =
          component.dataset.fakeRating || component.getAttribute('rating');

        return value == 1
          ? `1 Star`
          : `${value} Stars`;
      }

      const nameChain = ['Rating Stars', getHidden(), getType(), getSize(), getRating()];

      if (!isReadonly()) {
        nameChain.push(order(4, getStateName(component)));
      }

      component.dataset.sketchSymbol = nameChain.join(' / ');
    }
  );

  setTimeout(() => {
    onBeforePage2Layers()
  }, 2000);
</script>
