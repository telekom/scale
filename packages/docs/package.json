{
  "name": "@scaleds/docs",
  "description": "Scale Components Documentation",
  "version": "0.1.4",
  "author": "eeegor",
  "dependencies": {
    "@scaleds/gatsby-plugin": "^0.1.4",
    "@textlint/markdown-to-ast": "^6.1.7",
    "gatsby": "^2.17.11",
    "gatsby-image": "^2.2.31",
    "gatsby-plugin-manifest": "^2.2.27",
    "gatsby-plugin-offline": "^3.0.19",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-sharp": "^2.2.37",
    "gatsby-remark-prismjs": "^3.3.25",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-transformer-json": "^2.2.17",
    "gatsby-transformer-remark": "^2.6.34",
    "gatsby-transformer-sharp": "^2.3.3",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "uuid": "^7.0.2",
    "vfile-message": "^2.0.4"
  },
  "devDependencies": {
    "nodemon": "^1.19.4",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run components:sync && gatsby build",
    "postbuild": "yarn copy:dist",
    "copy:dist": "cp -R ./stencil/dist ./public/dist",
    "components:clean": "node ./scripts/clean.js",
    "components:copy": "node ./scripts/copy.js",
    "components:sync": "yarn components:clean && yarn components:copy",
    "components:update": "cd ../components && yarn build && cd ../docs && yarn components:sync && yarn copy:dist",
    "predevelop": "yarn build",
    "develop": "gatsby develop",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*",
      "stencil/*",
      "static/*"
    ],
    "delay": "1500"
  }
}
