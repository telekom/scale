{
  "name": "@telekom/scale-components-vue",
  "sideEffects": false,
  "version": "3.0.0-beta.158",
  "description": "Vue specific wrapper for @telekom/scale-components",
  "license": "MPL-2.0",
  "homepage": "https://github.com/telekom/scale",
  "repository": {
    "type": "git",
    "url": "https://github.com/telekom/scale.git"
  },
