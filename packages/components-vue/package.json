  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.8.0",
    "@types/node": "^16.0.0",
    "@vue/test-utils": "^1.2.0",
    "jest": "^26.1.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^26.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
