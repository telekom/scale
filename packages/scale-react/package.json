{
  "name": "@scaleds/scale-react",
  "sideEffects": false,
  "version": "0.1.2",
  "description": "React proxy for @scaleds/components",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf dist",
    "compile": "yarn tsc",
    "tsc": "tsc -p ."
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "@types/react-dom": "^16.9.6",
    "@types/vfile-message": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
